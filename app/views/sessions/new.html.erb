<div class="all">
<h1 class="h3 mb-3 font-weight-normal">Welcome! Please Sign In</h1>

<h4><%= flash[:notice] %></h4>

<%= form_tag(signin_path) do %>
  <%= label_tag :email %>
  <%= email_field_tag :email %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag 'Sign In' %><br>
<% end %>
<a href="http://localhost:3000/users/new"><button type="button" name="Sign Up">Sign Up</button></a><br>
</div>

<h1>Create My Account</h1>
<%= form_for @user do |f| %>
  <%= f.label :first_name %><br>
  <%= f.text_field :first_name %><br><br>
  <%= f.label :last_name %><br>
  <%= f.text_field :last_name %><br><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br><br>
  <%= f.submit "Create Account"%><br>
<% end %>
